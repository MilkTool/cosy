
struct Point {
	mut x Num = 2
    var y = 4
}

struct Test {
    var a Array Num
    var p Point
}

{
    var test = Test { a = [], p = Point { x = 5, y = 13 } }
    print test
}

{
    struct Test2 {
        var a Array Num
        var p Point = Point { x = 4, y = 12 }
    }

    var test = Test2 { a = [3, 4, 5] }
    print test
}
