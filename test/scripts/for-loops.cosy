fun f() {
    for j in 0..2 {
        print "loop #" + j
    }
    var from = 2
    var to = 4
    for j in from..to {
        print "loop #" + j
    }
}

fun f1() {
    mut i = 0
    for i < 5 {
        print "conditional loop #" + i
        i = i + 1
        if (i >= 3) return _
    }
}

fun f2() {
    mut j = 0
    for {
        print "infinite loop #" + j
        j = j + 1
        if (j >= 3) return _
    }
}

f()
f1()
f2()
