print 'for min..max'
for i in 0..4 {
    if i == 2 {
        print 'skip #2'
        continue
    }
    print i
}

print 'for array'
for i in [3,4,5] {
    if i == 4 {
        print 'skip value 4'
        continue
    }
    print i
}

print 'for'
mut i = 0
for i <= 3 {
    i += 1
    if i == 2 {
        print 'skip #2'
        continue
    }
    print i
}
